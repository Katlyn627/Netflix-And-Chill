{
  "expo": {
    "name": "Netflix & Chill",
    "slug": "netflix-and-chill",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.netflixandchill.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.netflixandchill.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "netflixandchill",
    "primaryColor": "#E50914"
  }
